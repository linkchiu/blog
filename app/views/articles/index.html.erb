<h1>Listing Articles</h1>
 <p><%= link_to 'Home', controller: 'welcome' %></p>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th></th>
  </tr>
 
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.text %></td>
      <td><%= link_to 'Show', article_path(article) %></td>
      <td><%= link_to 'edit1', edit_article_path(article) %></td>
    </tr>
  <% end %>
</table>